{
    "name": "orion",
    "version": "0.1.0",
    "private": true,
    "devDependencies": {
        "@tsconfig/node14": "^1.0.1",
        "@types/mocha": "^8.2.3",
        "@types/webpack": "^5.28.0",
        "@types/webpack-env": "^1.16.2",
        "@webpack-blocks/assets": "^2.1.0",
        "@webpack-blocks/core": "^2.1.0",
        "@webpack-blocks/dev-server": "^2.1.0",
        "@webpack-blocks/extract-text": "^2.1.0",
        "@webpack-blocks/postcss": "^2.1.0",
        "@webpack-blocks/uglify": "^2.1.0",
        "@webpack-blocks/webpack": "^2.1.0",
        "autoprefixer": "^10.3.0",
        "copy-webpack-plugin": "^9.0.1",
        "cross-env": "^7.0.3",
        "cyclejs-test-helpers": "^4.0.0",
        "html-webpack-harddisk-plugin": "^2.0.0",
        "html-webpack-plugin": "^5.3.2",
        "husky": "^7.0.1",
        "istanbul-instrumenter-loader-fix": "^3.0.2",
        "lint-staged": "^11.0.0",
        "mocha": "^9.0.2",
        "mocha-webpack": "^2.0.0-beta.0",
        "nyc": "^15.1.0",
        "postcss": "^8.3.5",
        "postcss-loader": "^6.1.1",
        "prettier": "^2.3.2",
        "rimraf": "^3.0.2",
        "snabbdom-looks-like": "^1.0.6",
        "snabbdom-pragma": "^2.8.0",
        "ts-loader": "^9.2.3",
        "typescript": "~4.3.5",
        "webpack": "^5.44.0",
        "webpack-cli": "^4.7.2",
        "webpack-dev-server": "^3.11.2",
        "webpack-merge": "^5.8.0",
        "webpack-node-externals": "^3.0.0"
    },
    "scripts": {
        "start": "cross-env NODE_ENV=development webpack serve --config configs/webpack.config.js",
        "test": "cross-env NODE_ENV=test nyc mocha-webpack --colors --webpack-config configs/webpack.config.js test/**/*.test.{js,jsx,ts,tsx}",
        "build": "cross-env NODE_ENV=production webpack --config configs/webpack.config.js",
        "clean": "rimraf build .nyc_output coverage"
    },
    "lint-staged": {
        "*.{js,jsx,ts,tsx}": [
            "prettier --write",
            "git add"
        ]
    },
    "prettier": {
        "tabWidth": 4,
        "singleQuote": true
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "nyc": {
        "instrument": false,
        "sourceMap": false,
        "include": [
            "src/components"
        ],
        "reporter": [
            "html",
            "text"
        ]
    },
    "dependencies": {
        "@cycle/dom": "^23.0.0",
        "@cycle/history": "^8.0.0",
        "@cycle/isolate": "^5.2.0",
        "@cycle/run": "^5.5.0",
        "@cycle/state": "^1.5.0",
        "@cycle/time": "^0.21.0",
        "cyclejs-utils": "^4.0.2",
        "cyclic-router": "^6.0.0",
        "switch-path": "^1.2.0",
        "xstream": "^11.14.0"
    },
    "one-fits-all": true,
    "browserslist": [
        "defaults"
    ],
    "engines": {
        "node": ">= 14"
    }
}
